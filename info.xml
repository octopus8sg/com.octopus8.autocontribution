<?xml version="1.0"?>
<extension key="com.octopus8.autocontribution" type="module">
  <file>autocontribution</file>
  <name>Auto Contribution</name>
  <description>Made with Ninja Forms in mind, this extension allows you to create Contributions with custom fields from ninja forms rather than manually apply it yourself.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Melissa</author>
    <email>melissa@octopus8.com | nadiya.m.madden@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Github">http://FIXME</url>
  </urls>
  <releaseDate>2023-12-20</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.66</ver>
  </compatibility>
  <comments>Modified the code for upgraded civicrm versions(until ver5.75.1) - may</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Autocontribution</namespace>
    <format>23.12.2</format>
    <angularModule>crmAutocontribution</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
